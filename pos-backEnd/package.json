{
  "name": "pos-backend",
  "version": "1.0.0",
  "description": "POS Management System Backend with TypeScript",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "seed": "ts-node src/utils/seedData.ts",
    "render-build": "npm install && npm run build"
  },
  "engines": {
    "node": "18.x",
    "npm": "9.x"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "cors": "2.8.5",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "express-rate-limit": "7.1.5",
    "express-validator": "7.0.1",
    "helmet": "7.0.0",
    "joi": "17.9.2",
    "jsonwebtoken": "9.0.2",
    "mongoose": "7.5.0",
    "multer": "1.4.5-lts.1",
    "slugify": "^1.6.6"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.6",
    "@types/cors": "2.8.17",
    "@types/dotenv": "8.2.0",
    "@types/express": "4.17.21",
    "@types/express-validator": "3.0.0",
    "@types/jsonwebtoken": "9.0.5",
    "@types/mongoose": "5.11.97",
    "@types/multer": "1.4.11",
    "@types/node": "20.10.4",
    "nodemon": "3.0.1",
    "ts-node": "10.9.1",
    "typescript": "5.3.2"
  },
  "keywords": [
    "pos",
    "backend",
    "nodejs",
    "express",
    "mongodb",
    "typescript"
